<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
	caption="msg://browseCaption" class="local.paxbase.web.site.PreferredSiteBrowse"
	messagesPack="local.paxbase.web.site">
	<dsContext>
		<groupDatasource id="sitesDs"
			class="local.paxbase.entity.coredata.Site" view="preferred-site-view">
			<query>
                <![CDATA[select e from paxbase$Site e]]>
			</query>
		</groupDatasource>
	</dsContext>
	<actions>
		<action id="openPopUp" caption="Open Sites" invoke="openPopUp" />
	</actions>
	<dialogMode height="600" width="800" />
	<layout spacing="true">
		<frame src="local/paxbase/web/userpreference/PreferredSites.xml"/>
		<button action="openPopUp" />
	</layout>
</window>
