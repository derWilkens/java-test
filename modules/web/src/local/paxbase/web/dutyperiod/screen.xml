<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="local.paxbase.web.dutyperiod.Screen"
        messagesPack="local.paxbase.web.dutyperiod">
    <dsContext>
        <valueCollectionDatasource id="newDs">
            <query>
                <![CDATA[SELECT max(e.endDate) as me from paxbase$DutyPeriod e]]>
            </query>
        </valueCollectionDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout>
        <textField datasource="newDs"
                   datatype="string"
                   property="me"/>
    </layout>
</window>
