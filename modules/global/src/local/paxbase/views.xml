<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="local.paxbase.entity.Transfer"
          extends="_local"
          name="transfer-browser-view"/>
    <view class="local.paxbase.entity.Transfer"
          extends="_local"
          name="transfer-edit-view">
        <property name="waypointList"
                  view="_minimal">
            <property name="site">
                <property name="client"/>
                <property name="siteName"/>
            </property>
            <property name="client"/>
        </property>
        <property name="crewChange"
                  view="_minimal">
            <property name="flightDate"/>
        </property>
        <property name="operatedBy"
                  view="_minimal"/>
        <property name="modeOfTransfer"
                  view="_minimal"/>
    </view>
    <view class="local.paxbase.entity.coredata.Site"
          extends="_local"
          name="site-browser-view">
        <property name="parentSite"
                  view="_minimal"/>
        <property name="siteType"
                  view="_minimal"/>
    </view>
    <view class="local.paxbase.entity.DutyPeriod"
          extends="_local"
          name="dutyPeriod-edit-view">
        <property name="site"
                  view="_minimal"/>
        <property name="outboundTransfer"
                  view="_minimal"/>
        <property name="inboundTransfer"
                  view="_minimal"/>
        <property name="contractor"
                  view="_minimal"/>
        <property name="personOnDuty"
                  view="_minimal"/>
    </view>
    <view class="local.paxbase.entity.OffshoreUser"
          name="offshoreUser-browser-view">
        <property name="company"
                  view="_minimal">
        </property>
        <property name="position"/>
        <property name="firstName"/>
        <property name="lastName"/>
    </view>
    <view class="local.paxbase.entity.OffshoreUser"
          name="offshoreUser-edit-view">
        <property name="company"
                  view="_minimal">
            <property name="companyName"/>
        </property>
        <property name="login"/>
        <property name="password"/>
        <property name="firstName"/>
        <property name="lastName"/>
        <property name="position"/>
        <property name="email"/>
        <property name="language"/>
        <property name="active"/>
        <property name="changePasswordAtNextLogon"/>
        <property name="group"
                  view="_minimal"/>
        <property name="client"/>
    </view>
    <view class="local.paxbase.entity.Waypoint"
          extends="_local"
          name="waypoint-browser-view">
        <property name="site"
                  view="_minimal"/>
    </view>
    <view class="local.paxbase.entity.Campaign"
          extends="_local"
          name="campaign-view">
        <property name="site"
                  view="_minimal"/>
        <property name="category"
                  view="_minimal"/>
    </view>
    <view class="local.paxbase.entity.coredata.Site"
          extends="_local"
          name="siteCampaignsView">
        <property name="campaigns"
                  view="_minimal">
        </property>
    </view>
    <view class="local.paxbase.entity.Campaign"
          extends="_local"
          name="campaignOverview">
        <property name="site"
                  view="_minimal"/>
        <property name="category"
                  view="_minimal"/>
    </view>
    <view class="local.paxbase.entity.coredata.Site"
          extends="_local"
          name="siteSelect"/>
    <view class="local.paxbase.entity.UserPreference"
          extends="_local"
          name="userPreference-view"/>
    <view class="local.paxbase.entity.coredata.PeriodType"
          extends="_local"
          name="periodType-view">
        <property name="parentType"
                  view="_minimal">
        </property>
    </view>
    <view class="local.paxbase.entity.coredata.Site"
          extends="_local"
          name="site-view"/>
    <view class="local.paxbase.entity.coredata.FunctionCategory"
          name="functionCategory-treeView">
        <property name="parentType"
                  view="_minimal"/>
        <property name="categoryName"/>
        <property name="periodSubClass"/>
    </view>
    <view class="local.paxbase.entity.coredata.FunctionCategory"
          name="functionCategory-view">
        <property name="categoryName"/>
        <property name="parentType"
                  view="_minimal"/>
        <property name="periodSubClass"/>
    </view>
    <view class="local.paxbase.entity.coredata.SiteType"
          extends="_local"
          name="siteType-view">
        <property name="parentType"
                  view="_minimal">
            <property name="type"/>
        </property>
    </view>
</views>
